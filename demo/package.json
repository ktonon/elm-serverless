{
  "name": "elm-serverless-demo",
  "version": "1.0.0",
  "engines": {
    "node": ">=6.10.0"
  },
  "description": "Small demonstrative AWS Lambda API written in elm",
  "author": "Kevin Tonon <kevin@betweenconcepts.com>",
  "license": "MIT",
  "repository": "git@github.com:ktonon/elm-serverless.git",
  "scripts": {
    "deploy": "cross-env NODE_ENV=production serverless deploy",
    "postinstall": "elm package install -y && cd .. && npm install",
    "start": "serverless offline",
    "test-server": "serverless offline --port=3001 > test-server.log 2>&1",
    "test": "mocha test"
  },
  "devDependencies": {
    "cross-env": "^3.1.4",
    "elm": "^0.18.0",
    "elm-webpack-loader": "^4.3.1",
    "mocha": "^3.4.2",
    "rc": "^1.1.6",
    "serverless": "^1.17.0",
    "serverless-offline": "^3.15.1",
    "serverless-webpack": "^2.0.0",
    "shebang-loader": "0.0.1",
    "should": "^11.2.1",
    "strip-debug-loader": "^1.0.0",
    "superagent-defaults": "^0.1.14",
    "supertest": "^3.0.0",
    "webpack": "^3.3.0"
  }
}
